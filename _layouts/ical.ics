BEGIN:VCALENDAR
VERSION:2.0
PRODID:{{ current_page.data.cal_prodid }}
NAME:{{ current_page.data.cal_name }}
X-WR-CALNAME:{{ current_page.data.cal_name }}
DESCRIPTION:{{ current_page.data.cal_description }}
X-WR-CALDESC:{{ current_page.data.cal_description }}
{% for event in current_page.data.events %}BEGIN:VEVENT
ORGANIZER;{{ event.organizer }}
UID:{{ event.uid }}
DTSTAMP:{{ event.dtstamp }}
DTSTART:{{ event.dtstamp }}
DTEND:{{ event.dtstamp }}
SUMMARY:{{ event.summary }}
DESCRIPTION:{{ event.description }}
LOCATION:{{ event.location }}
END:VEVENT{% endfor %}
END:VCALENDAR
